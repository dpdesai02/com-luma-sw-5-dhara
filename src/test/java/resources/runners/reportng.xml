<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1" >
    <listeners>
        <listener class-name="com.softwaretestingboard.magento.customerListeners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name="Gear Test" >
        <classes>
            <class name="com.softwaretestingboard.magento.testsuite.GearTest"/>
        </classes>
    </test>
    <test name="Men Test" >
        <classes>
            <class name="com.softwaretestingboard.magento.testsuite.MenTest"/>
        </classes>
    </test>
    <test name="Women Test" >
        <classes>
            <class name="com.softwaretestingboard.magento.testsuite.WomenTest"/>
        </classes>
    </test>
</suite>